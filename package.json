{
  "name": "awesome-lists",
  "version": "1.6.1",
  "description": "Awesome Lists and Resources",
  "author": "PatEvs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patevs/awesome-lists.git"
  },
  "scripts": {
    "commit": "git-cz",
    "release": "np",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/core": "^9.0.1",
    "commitizen": "^4.1.2",
    "commitlint-config-gitmoji": "^1.0.1",
    "cz-emoji": "^1.2.1",
    "husky": "^4.2.5",
    "np": "^6.3.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "./node_modules/commitlint-config-gitmoji"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    },
    "cz-emoji": {
      "skipQuestions": [
        "body",
        "issues"
      ]
    }
  }
}
