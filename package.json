{
  "name": "awesome-lists",
  "version": "1.17.0",
  "description": "Awesome Lists and Resources",
  "author": "PatEvs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patevs/awesome-lists.git"
  },
  "scripts": {
    "updates": "npx ncu",
    "release": "np"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "commitlint-config-gitmoji": "^2.2.3",
    "husky": "^4.3.8",
    "np": "^7.4.0"
  },
  "commitlint": {
    "extends": [
      "gitmoji"
    ],
    "rules": {
      "type-empty": [
        0,
        "never"
      ],
      "subject-empty": [
        0,
        "never"
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
